local dsinv = "https://discord.com/invite/tjGyr7YeGC" local ms2id = 9551640993 if game.PlaceId == ms2id then local plicon = "http://www.roblox.com/asset/?id=6023426915" local txticon = "http://www.roblox.com/asset/?id=6022668888" local Library = loadstring(game:HttpGet"https://raw.githubusercontent.com/Loloresh/LXWare/main/Library")() local Window = Library:Window("LXWare", "Mining Simulator 2", Color3.fromRGB(255, 110, 48), Enum.KeyCode.RightShift) local Main = Window:Tab('Main',txticon) Main:Label('Place supported! [Mining Simulator 2]') Main:Label('RShift to hide UI') Main:Label('Contact us if you find any bug!') Main:Button("Copy dev's contact", "This button will copy dev's discord contact to your clipboard", function() setclipboard('lxresh#7226') Library:Notification("Dev's discord contact successfully copied!", "Ok") end) Main:Button("Copy our discord server invite", "This button will copy our discord server invite to your clipboard", function() setclipboard(dsinv) Library:Notification("Our discord server successfully copied!", "Ok") end) local Misc = Window:Tab('Fun',plicon) Misc:Button('Hide/Show HUD','Btw you can use this function to record any cinematics idk',function()
if game:GetService("Players").efni21.PlayerGui.ScreenGui.HUD.Visible == true then game:GetService("Players").efni21.PlayerGui.ScreenGui.HUD.Visible = false else game:GetService("Players").efni21.PlayerGui.ScreenGui.HUD.Visible = true end end) Misc:Slider('WalkSpeed','Makes you walk faster (16 by default)', 8, 30, 16,function(s) game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = s end) Misc:Slider('Gravity','Changes your gravity', 0, 250, 125,function(j) game.Workspace.Gravity = j  end) Misc:Slider('Air Walking Height','The distance between you and floor',3,250,3, function(hght) game:GetService('Players').LocalPlayer.Character.Humanoid.HipHeight = hght end) Misc:Button("Disable Air Walk",'Allows you to walk on Air',function() game:GetService('Players').LocalPlayer.Character.Humanoid.HipHeight = 2.22 end) Misc:Button('Noclip','Allows you to walk through walls', function() Library:Notification("Noclip successfully enabled!","Ok") loadstring(game:HttpGet'https://pastebin.com/raw/8Rd1r22a')() end) Misc:Button('InfJump','Allows you to infinite spam jump',function()	 Library:Notification("InfJump successfully enabled!","Ok") local Infjumpenabled = true game:GetService("UserInputService").JumpRequest:connect(function() if Infjumpenabled then game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping") end end) end) Misc:Button('Show Hitbox on/off','Changes "HumanoidRootPart" Opacity',function() if game:GetService("Workspace").efni21.HumanoidRootPart.Transparency == 0 then game:GetService("Workspace").efni21.HumanoidRootPart.Transparency =	100 else game:GetService("Workspace").efni21.HumanoidRootPart.Transparency = 0 end end) Misc:Dropdown('Hitbox type',{'Ball','Block','Cylinder'},function(type) if type == "Ball" then game:GetService("Workspace").efni21.HumanoidRootPart.Shape = 'Ball'	elseif type == 'Block' then game:GetService("Workspace").efni21.HumanoidRootPart.Shape = 'Block' elseif type == 'Cylinder' then game:GetService("Workspace").efni21.HumanoidRootPart.Shape = 'Cylinder' end end) Misc:Button("Sit on/off",'Makes you sit idk :D', function() if game:GetService('Players').LocalPlayer.Character.Humanoid.Sit == false then game:GetService('Players').LocalPlayer.Character.Humanoid.Sit = true else game:GetService('Players').LocalPlayer.Character.Humanoid.Sit = false end end) local Tab = Window:Tab('Character',plicon) Tab.Colorpicker("ESP Color",Color3.fromRGB(255,1,1), function(t) print(t) end) local Updates = Window:Tab('PatchNote',txticon) Updates:Label('Update 2:') Updates:Label('New Supported Place! [Mining Simulator 2]') Updates:Label('And a lot of new functions :)') else game.Players.LocalPlayer:Kick("Probably place not supported | Join Discord for feature list link copied. "..dsinv) setclipboard(dsinv) end
